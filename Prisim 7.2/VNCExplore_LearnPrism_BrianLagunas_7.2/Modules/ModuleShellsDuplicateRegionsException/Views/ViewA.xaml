<UserControl x:Class="ModuleShellsDuplicateRegionsException.ViewA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://www.codeplex.com/prism"
             xmlns:infra="clr-namespace:VNCExplore_LearnPrism_BrianLagunas.Infrastructure;assembly=VNCExplore_LearnPrism_BrianLagunas.Infrastructure"

             prism:ViewModelLocator.AutoWireViewModel="True"
             Background="LightGray">
    <StackPanel >
        <TextBlock Text="ViewA" FontSize="32" HorizontalAlignment="Center" VerticalAlignment="Center" />
        <ContentControl prism:RegionManager.RegionName="{x:Static infra:RegionNames.ChildRegion}" Margin="15" />
    </StackPanel>
</UserControl>
